	olive:exampleTriple ":Situation1 olive:situationHasAssociatedMonetaryValue [olive:decimalValue \"500\"^^xsd:double ; olive:hasUoM olive:_USDollar ]" ;
.

olive:sold a owl:ObjectProperty ;
